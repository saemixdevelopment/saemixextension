@article{koo99,
author="Koo, Ja-Yong",
title="{Logspline deconvolution in Besov space.}",
language="English",
journal="Scand. J. Stat. ",
volume="26",
number="1",
pages="73-86",
year="1999",
abstract="{The author proposes a method of estimation of the unknown density
    function $f_X$ when the data are contaminated with an additive
    noise, i.e. instead of observing the random variable $X$ directly
    one observe $Y=X+Z$, where $Z$ is a noise with a known density
    $f_Z$ and is distributed independently of $X$. B-splines and
    the Fourier inversion formula are used to construct the logspline
    density estimator $f^*$ of $f_X$. Asymptotic properties of $f^*$
    and rates of convergence to $f_X$ are established when log-density
    function belongs to a Besov space. It is proved that convergence
    rates depend on smoothness of the density function and the decay
    rate of the characteristic function of the noise. Simulated data
    illustrates the finite sample performance of logspline density
    estimations.}",
reviewer="{N.M.Zinchenko (Kyïv)}",
keywords="{contaminated data; convolution equation; B-splines; logspline density estimation; Fourier transform; Besov space; convergence; Fourier inversion formula}",
classmath="{*65C99 Numerical simulation
62F12 Asymptotic properties of parametric estimators
62F10 Point estimation}",
}

@article{koo98,
author="Koo, Ja-Yong and  Chung, Han-Yeong",
title="{Log-density estimation in linear inverse problems.}",
language="English",
journal="Ann. Stat. ",
volume="26",
number="1",
pages="335-362",
year="1998",
abstract="{We estimate a probability density function $p$ which is related
    by a linear operator $K$ to a density function $q$ in sequences
    of regular exponential families based on a random sample from
    $q$. Deconvolution and positron emission tomography are considered.
    The logarithm of the density function is approximated by basis
    functions consisting of singular functions of $K$. While direct
    maximum likelihood (or minimum Kullback-Leibler) density estimation
    in exponential families selects the parameters to match the moments
    of the basis functions to the sample moments, in the inverse
    problem the moment of each singular function is related to a
    corresponding moment of the direct problem by a factor given
    by a singular value $\lambda_\nu$ of $K$. Thus an appropriate
    analogue of the maximum likelihood estimate is obtained by matching
    moments with respect to $p$ to $1/\lambda_\nu$ times the empirical
    moments associated with the sample from $q$.\par
Bounds on the
    Kullback-Leibler distance between the true density and the estimators
    are obtained and rates of convergence are established for log-density
    functions having a measure of smoothness. The density estimator
    converges to the unknown density in the Kullback-Leibler sense
    and in the $L_2$-sense at a rate determined not only by the order
    of smoothness of the log-density and the dimension of data but
    also by the decay rate of the singular values of the operator.
    A minimax lower bound for deconvolution is provided under certain
    conditions. Numerical examples using simulated data are provided
    to illustrate the finite-sample performance of the proposed method
    for deconvolution and positron emission tomography.}",
keywords="{linear inverse problems; SVD; indirect likelihood; MILE; Kullback-Leibler distance; rate of convergence; exponential families; positron emission tomography}",
classmath="{*62G07 Curve estimation
62P99 Appl. of statistics
62G05 Nonparametric estimation
62P35 Applications of statistics to physics}",
}


@article{koo96,
author="Koo, Ja-Yong and  Park, Byeong U.",
title="{$B$-spline deconvolution based on the EM algorithm.}",
language="English",
journal="J. Stat. Comput. Simulation ",
volume="54",
number="4",
pages="275-288",
year="1996",
abstract="{$B$-splines are considered for the deconvolution problem of estimating
    a probability density function when the sample observations are
    contaminated with random noise. In the logspline method of density
    estimation, the logarithm of the unknown density function is
    approximated by a polynomial spline, the unknown parameters of
    which are estimated by maximum likelihood. Based on the logspline
    method, a fully automated procedure involving the EM algorithm,
    stepwise knot deletion and BIC has been developed for deconvolution.
    Numerical examples using simulated data are given to show the
    performance of the $B$-spline deconvolution.}",
keywords="{B-splines; model selection; deconvolution; density estimation; logspline method; automated procedure; EM algorithm; stepwise knot deletion}",
classmath="{*62G07 Curve estimation
65C99 Numerical simulation}",
}



@article{card02,
author="Cardot, Hervé",
title="{Spatially adaptive splines for statistical linear inverse problems.}",
language="English",
journal="J. Multivariate Anal. ",
volume="81",
number="1",
pages="100-119",
year="2002",
abstract="{Author's abstract: This paper introduces a new nonparametric
    estimator based on penalized regression splines for linear operator
    equations when the data are noisy. A local roughness penalty
    that relies on local support properties of B-splines is introduced
    in order to deal with spatial heterogeneity of the function to
    be estimated. This estimator is shown to be consistent under
    weak conditions on the asymptotic behaviour of the singular values
    of the linear operator. Furthermore, in the usual non-parametric
    settings, it is shown to attain optimal rates of convergence.
    Then its good performances are confirmed by means of a simulation
    study.}",
reviewer="{C.W.Groetsch (Cincinnati)}",
keywords="{statistical linear inverse problems; ill-posed problem; nonparametric estimator; penalized regression splines; linear operator equations; B-splines; singular values; convergence; performances}",
